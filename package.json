{
  "name": "table-link",
  "version": "1.2.2",
  "description": "A simple way to add links on your tableish elements like tr, th and td.",
  "main": "table-link.js",
  "scripts": {
    "build": "npm run build-js && npm run build-min",
    "build-js": "browserify ./src/TableLink.js -s TableLink -o ./table-link.js",
    "watch": "watchify ./src/TableLink.js -s TableLink -o ./table-link.js",
    "build-min": "uglifyjs ./table-link.js -o ./table-link.min.js",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brulivera/table-link.git"
  },
  "keywords": [
    "table",
    "link",
    "datatable"
  ],
  "author": "Bruno Oliveira <bruliveira0@gmail.com> (https://github.com/brulivera)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brulivera/table-link/issues"
  },
  "homepage": "https://github.com/brulivera/table-link#readme",
  "dependencies": {
    "delegate": "^3.0.1",
    "matches-selector": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "browserify": "^13.0.0",
    "jsdom": "^9.5.0",
    "simulant": "^0.2.2",
    "sinon": "^1.17.6",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*"
    ],
    "require": [
      "./test/helpers/browser-mock.js"
    ]
  }
}
